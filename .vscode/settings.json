{
    "rust-analyzer.checkOnSave.overrideCommand": [
        "cargo",
        "offline",
        "check",
        "--workspace",
        "--message-format=json",
        "--features=cargo-metadata"
    ],
    "rust-analyzer.cargo.buildScripts.overrideCommand": [
        "cargo",
        "offline",
        "check",
        "--quiet",
        "--workspace",
        "--message-format=json",
        "--all-targets",
        "--features=cargo-metadata"
    ],
    "rust-analyzer.cargo.features": [
        "cargo-metadata"
    ]
}